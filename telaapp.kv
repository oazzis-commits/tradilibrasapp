<TelaPrincipal>:
    MeuLayout:
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size
                source: "imagens/fundotudo.png"
        Widget:
            size_hint: 0.78, 1   # ocupa tanana da tela
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            canvas.before:
                Color:
                    rgba: 0.945, 0.925, 0.902, 0.6  # cor f1ece6 com 60% transparência
                Rectangle:
                    pos: self.pos
                    size: self.size

        BoxLayout:
            MeuLayout:
                Label:
                    text: "TradiLibras"
                    font_name: "fontes/titulo.ttf"
                    id: trad
                    pos_hint: {'x': .25, 'y': .8}
                    size_hint: .5, .1
                    font_size: 80
                Image:
                    source: "imagens/logoapp.png"
                    size_hint: 0.15, 0.15
                    pos_hint: {'x': 0.6, 'y': 0.78}

                FloatLayout:

            # botao principal oqelibras
                Button:
                    text: "O que é Libras?"
                    font_size: 18
                    id: oqe
                    size_hint: .6, None
                    height: 50
                    pos_hint: {'x': .209, 'y': .66}
                    background_normal: "imagens/fundobotoes.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    color: 1,1,1,1
                    on_release:
                        app.mudar_tela('oqelibras')

            # botao circular oqelibras
                Button:
                    text: ""
                    size_hint: None, None
                    size: 80, 80
                    pos_hint: {'x': .17, 'y': .638}
                    background_normal: "imagens/iconoqelibras.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    border: (0,0,0,0)
                    on_release: app.mudar_tela('oqelibras')

                Label:
                    text: "[b]Sinalização:[/b]"
                    markup: True
                    id: sin
                    font_size: 20
                    size_hint: .6, None
                    height: 50
                    pos_hint: {'x': .209, 'y': .575}

            # botao principal alfabeto
                Button:
                    text: "Alfabeto"
                    font_size: 18
                    id: alf
                    size_hint: .6, None
                    height: 50
                    pos_hint: {'x': .209, 'y': .501}
                    background_normal: "imagens/fundobotoes.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    color: 1,1,1,1
                    on_release:
                        app.mudar_tela('alfabeto')

            # botao circular alfabeto
                Button:
                    text: " "
                    size_hint: None, None
                    size: 60, 60
                    pos_hint: {'x': .175, 'y': .493}
                    background_normal: "imagens/iconalfabeto.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    border: (0,0,0,0)
                    on_release:
                        app.mudar_tela('alfabeto')

            # botao principal saudacoes
                Button:
                    text: "Saudações"
                    font_size: 18
                    id: sau
                    size_hint: .6, None
                    height: 50
                    pos_hint: {'x': .209, 'y': .416}
                    background_normal: "imagens/fundobotoes.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    color: 1,1,1,1
                    on_release:
                        app.mudar_tela('saudacoes')

            # botao circular saudacoes
                Button:
                    text: " "
                    size_hint: None, None
                    size: 60, 60
                    pos_hint: {'x': .175, 'y': .4072}
                    background_normal: "imagens/iconsaudacoes.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    border: (0,0,0,0)
                    on_release:
                        app.mudar_tela('saudacoes')

                Label:
                    text: "[b]Dúvidas?[/b]"
                    markup: True
                    id: duv
                    font_size: 20
                    size_hint: .6, None
                    height: 50
                    pos_hint: {'x': .209, 'y': .333}

            # botao principal tradutor
                Button:
                    text: "Tradução instantânea de sinais"
                    font_size: 18
                    size_hint: .6, None
                    height: 50
                    pos_hint: {'x': .209, 'y': .258}
                    id: tra
                    background_normal: "imagens/fundobotoes.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    color: 1,1,1,1
                    on_release:
                        app.mudar_tela('tradutor')

            # botao circular tradutor
                Button:
                    text: " "
                    size_hint: None, None
                    size: 60, 60
                    pos_hint: {'x': .175, 'y': .251}
                    background_normal: "imagens/icontradutor.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    border: (0,0,0,0)
                    on_release:
                        app.mudar_tela('tradutor')

            # botao principal praticar
                Button:
                    text: "Praticar"
                    font_size: 18
                    size_hint: .6, None
                    height: 50
                    pos_hint: {'x': .209, 'y': .174}
                    id: pra
                    background_normal: "imagens/fundobotoes.png"
                    background_down: "imagens/fundobotoes_pressed.png"
                    on_release:
                        app.mudar_tela('praticar')

            # botao circular praticar
